          "rawSql": "select bill_month as \"time\",service,\r\n  (CASE WHEN DATE_FORMAT(bill_month,'%Y-%m-%d') >= DATE_FORMAT(FROM_UNIXTIME(${__from}/1000) + INTERVAL 1 MONTH, '%Y-%m-15') THEN (sum(unblended_cost)  - (lag(sum(unblended_cost),1,0) over (partition by service order by bill_month))) ELSE 0 END) as \"上月环比\"\r\nfrom aws_bill_resource_summary\r\nwhere $__timeFilter(bill_month) and bill_account_id = \"${bill_account}\"\r\ngroup by bill_month,service\r\norder by bill_month",
