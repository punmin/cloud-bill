          "format": "time_series",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  bill_month AS \"time\",\r\n  bill_account_alias,\r\n  (CASE WHEN DATE_FORMAT(bill_month,'%Y-%m-%d') >= DATE_FORMAT(FROM_UNIXTIME(${__from}/1000) + INTERVAL 1 MONTH, '%Y-%m-15') THEN (sum(real_cost_cny)  - (lag(sum(real_cost_cny),1,0) over (partition by bill_account_alias order by bill_month))) ELSE 0 END)\r\n   as \"上月环比\"\r\nFROM all_bill_resource_summary\r\nWHERE\r\n  $__timeFilter(bill_month)\r\ngroup by bill_month,bill_account_alias\r\nORDER BY bill_month",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "id"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "aliyun_bill_resource_summary",
          "timeColumn": "bill_month",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "云平台应付款上月环比（负数即下降，正数即上升），请忽略第一个月份的数据",
